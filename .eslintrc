{
    "extends": ["airbnb", "airbnb/hooks", "eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "root": true,
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-console": "off",
        "semi": [2, "always"]
    }
}
